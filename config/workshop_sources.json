{
  "sources": [
    {
      "id": "voiceactorsnetwork",
      "name": "Voice Actors Network",
      "url": "https://voiceactorsnetwork.com/collections/all",
      "extractor": "van_shopify_products"
    },
    {
      "id": "thevopros",
      "name": "The VO Pros",
      "url": "https://www.thevopros.com/events/",
      "extractor": "thevopros_events_index"
    },

    {
      "id": "redscythe_tidycal_classes",
      "name": "Red Scythe Studio",
      "url": "https://tidycal.com/redscythestudio/",
      "extractor": "redscythe_tidycal",
      "timezone": "America/Los_Angeles",
      "headers": {
        "User-Agent": "Mozilla/5.0"
      },
      "filters": {
        "exclude_title_contains": [
          "coaching",
          "coach",
          "consult",
          "consultation",
          "1:1",
          "one-on-one",
          "one on one",
          "freelance",
          "marketing",
          "download",
          "video"
        ],
        "exclude_title_regex": [
          "(?i)consult",
          "(?i)coach(ing)?",
          "(?i)one\\s*-?on\\s*-?one",
          "(?i)\\b1\\s*:\\s*1\\b"
        ],
        "require_title_contains_any": [
          "class",
          "workshop",
          "voice",
          "vo",
          "adr",
          "dubbing",
          "commercial",
          "animation",
          "anime"
        ]
      }
    },

    {
      "id": "redscythe",
      "name": "Red Scythe",
      "url": "https://redscythe.com/",
      "extractor": "jsonld_events"
    },
    {
      "id": "soundonstudio",
      "name": "Sound On Studio",
      "url": "https://www.soundonstudio.com/classsignup",
      "extractor": "soundonstudio_classsignup"
    },
    {
      "id": "halp_academy",
      "name": "HALP Academy",
      "url": "https://halpacademy.com/events/search/",
      "extractor": "halp_events_search"
    },
    {
      "id": "voicetraxwest",
      "name": "Voice Trax West",
      "url": "https://www.voicetraxwest.com/guest-instructors",
      "extractor": "jsonld_events"
    },
    {
      "id": "adventuresinvoiceacting",
      "name": "Adventures in Voice Acting",
      "url": "https://www.adventuresinvoiceacting.com/",
      "extractor": "wix_service_list"
    },
    {
      "id": "realvoicela",
      "name": "Real Voice LA",
      "url": "https://www.realvoicela.com/classes",
      "extractor": "wix_service_list"
    },
    {
      "id": "thevodojo",
      "name": "The VO Dojo",
      "url": "https://www.thevodojo.com/upcoming-events-nav",
      "extractor": "vodojo_upcoming"
    }
  ],
  "settings": {
    "max_events_per_source": 50,
    "prune_days_past": 7,
    "keep_days_future": 365
  }
}
